{
  "name": "thetrago",
  "license": "0BSD",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "postinstall": "patch-package",
    "build:android-16kb": "npx expo prebuild --clean --platform android && eas build --platform android --profile production",
    "build:android-15": "cross-env ANDROID_16KB_PAGE_SIZE=true npx expo prebuild --clean --platform android",
    "clean:android": "cd android && ./gradlew clean && cd ..",
    "clean:cache": "npx expo start --clear",
    "clean:all": "rm -rf node_modules && rm -rf .expo && rm -rf android/build && rm -rf android/app/build && npm install"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/cli": "latest",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-google-signin/google-signin": "^15.0.0",
    "@react-native-picker/picker": "2.11.1",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.15",
    "@react-navigation/stack": "^7.1.1",
    "axios": "^1.8.4",
    "expo": "~53.0.20",
    "expo-apple-authentication": "~7.2.4",
    "expo-av": "~15.1.7",
    "expo-blur": "~14.1.4",
    "expo-build-properties": "~0.14.8",
    "expo-camera": "~16.1.11",
    "expo-dev-client": "~5.2.4",
    "expo-file-system": "~18.1.10",
    "expo-image-manipulator": "~13.1.6",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "~14.1.4",
    "expo-linking": "~7.1.7",
    "expo-location": "~18.1.6",
    "expo-mail-composer": "~14.1.5",
    "expo-media-library": "~17.1.6",
    "expo-network": "~7.1.5",
    "expo-print": "~14.1.4",
    "expo-secure-store": "~14.2.3",
    "expo-sharing": "~13.1.5",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "jsqr": "^1.4.0",
    "lottie-react-native": "7.2.2",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.47",
    "moti": "^0.30.0",
    "qrcode": "^1.5.4",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-calendars": "^1.1311.0",
    "react-native-curved-bottom-bar": "^3.4.1",
    "react-native-fbsdk-next": "^13.4.1",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-modal-datetime-picker": "^18.0.0",
    "react-native-network-info": "^5.2.1",
    "react-native-omise": "^0.0.2",
    "react-native-paper": "^5.13.5",
    "react-native-picker": "^4.3.7",
    "react-native-reanimated": "~3.17.4",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-skeleton-placeholder": "^5.2.4",
    "react-native-snap-carousel": "^1.3.1",
    "react-native-vector-icons": "^10.2.0",
    "react-native-webview": "^13.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@types/react": "~19.0.10",
    "patch-package": "^8.0.0",
    "typescript": "~5.8.3"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-fs",
          "react-native-responsive-screen",
          "react-native-skeleton-placeholder",
          "react-native-snap-carousel"
        ],
        "listUnknownPackages": false
      }
    }
  }
}
