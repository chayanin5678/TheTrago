{
  "cli": {
    "version": ">= 15.0.13",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "releaseChannel": "default",
        "compileSdkVersion": 35,
        "targetSdkVersion": 35,
        "minSdkVersion": 24
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "releaseChannel": "default",
        "compileSdkVersion": 35,
        "targetSdkVersion": 35,
        "minSdkVersion": 24
      }
    },
    "production": {
      "autoIncrement": false,
      "android": {
        "buildType": "app-bundle",
        "releaseChannel": "default",
        "compileSdkVersion": 35,
        "targetSdkVersion": 35,
        "minSdkVersion": 24,
        "gradleCommand": ":app:bundleRelease",
        "image": "latest",
        "env": {
          "EXPO_USE_FAST_RESOLVER": "1"
        }
      },
      "ios": {
        "autoIncrement": false
      }
    },
    "production-new": {
      "autoIncrement": false,
      "ios": {
        "autoIncrement": false
      }
    }
  },
  "submit": {
    "production": {}
  }
}
