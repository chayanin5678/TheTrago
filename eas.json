{
  "cli": {
    "version": ">= 15.0.13",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "releaseChannel": "default"
      },
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "releaseChannel": "default"
      },
      "ios": {
        "buildConfiguration": "Release"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle",
        "releaseChannel": "default"
      },
      "ios": {
        "buildConfiguration": "Release",
        "cache": {
          "disabled": true
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}
